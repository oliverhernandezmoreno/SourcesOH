    "build-doc": "node index.js build-doc && redoc-cli bundle openapi.built.json -o doc.html"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "commander": "^2.19.0",
    "express": "^4.16.4",
    "raven": "^2.6.4",
    "redis": "^2.8.0",
    "redoc-cli": "^0.9.8",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.9",
    "jest": "^23.6.0",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  }
}
