  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "find lib -regex \".*\\.ts\" | xargs tsc --lib ES2019 --target ES2019 --module commonjs index.ts",
    "lint": "tslint -c tslint.json index.ts 'lib/**/*.ts'",
    "start": "node index.js",
